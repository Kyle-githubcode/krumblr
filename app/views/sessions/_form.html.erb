<%= form_with(local: true, url: login_path) do |form| %>

    <p>
        <%= form.label :email %><br>
        <%= form.text_field :email %>
    </p>
    <p>
        <%= form.label :password %><br>
        <%= form.text_field :password, type: "password" %>
    </p>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>